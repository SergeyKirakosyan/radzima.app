<%
var langs = Object.keys(lang).filter(el => el !== locale);
var pattern = '^(\/' + locale + ')';
var reg = new RegExp(pattern, '');
%>
<% langs.forEach((lang) => { %>
<link href="<%= site.url[process.env.ENV || 'dev'] %>/<%= page.url.replace(reg, lang) %>" rel="alternate" hreflang="<%= lang %>">
<% }); %>
